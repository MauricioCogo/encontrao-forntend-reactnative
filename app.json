{
  "expo": {
    "name": "EncontraoXXX",
    "slug": "econtrao",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/TrempeIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBBcvJ28TdKx6wOk3feqWRtKRc17S5c9xk"
      },
      "bundleIdentifier": "com.mauriciocogo.EcontraoXXX"
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECORD_AUDIO"
      ],
      "useCleartextTraffic": true,
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Trempe.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBBcvJ28TdKx6wOk3feqWRtKRc17S5c9xk"
        }
      },
      "package": "com.mauriciocogo.EcontraoXXX"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        },
        "ios": {
        }
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "c8aed8c6-fc8f-460b-b4c8-f9e892fb43ee"
      }
    },
    "owner": "mauriciocarvalho"
  }
}
